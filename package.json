{
  "name": "@wyeg/logsy",
  "version": "0.0.1",
  "description": "Logsy - is a library for stylized logging of messages in the browser console. It allows developers to easily output structured and visual various messages with custom styles, tags and plugin support.",
  "main": "dist/logsy.umd.js",
  "module": "dist/logsy.es.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "vite build && tsc --emitDeclarationOnly",
    "prepublishOnly": "yarn build",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix"
  },
  "keywords": [
    "console",
    "log",
    "styled"
  ],
  "author": "WYEG, Tagir Khadzhiev",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "9.29.0",
    "@types/jest": "30.0.0",
    "@types/node": "24.0.14",
    "@typescript-eslint/eslint-plugin": "8.35.0",
    "@typescript-eslint/parser": "8.35.0",
    "eslint": "9.29.0",
    "eslint-plugin-import": "2.32.0",
    "jest": "29.7.0",
    "ts-jest": "29.4.0",
    "ts-node": "10.9.2",
    "typescript": "5.8.3",
    "typescript-eslint": "8.35.0",
    "vite": "7.0.0"
  }
}
